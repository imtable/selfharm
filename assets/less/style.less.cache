  margin-left: 15px;
  font-size: 20px;
}
.header .header_wrapper .header_rightside .header_links .link-git {
  display: flex;
  width: 80px;
}
.header .header_wrapper .header_rightside .header_links .link-git:hover {
  transform: scale(1.05);
}
.main {
  flex: 1 1 auto;
}
.main .container .task {
  display: flex;
  height: 100vh;
  width: 100%;
}
.main .container .task .left-wrapper {
  background: lightblue;
  height: 100vh;
  width: 70%;
  display: flex;
  justify-content: flex-end;
  padding: 5px 10px;
}
@media (max-width: 790px) {
  .main .container .task .left-wrapper {
    width: 100%;
  }
}
.main .container .task .left-wrapper .left-box_wrapper {
  width: 900px;
  height: 500px;
}
@media (max-width: 1350px) {
  .main .container .task .left-wrapper .left-box_wrapper {
    width: 700px;
  }
}
@media (max-width: 1060px) {
  .main .container .task .left-wrapper .left-box_wrapper {
    width: 500px;
  }
}
@media (max-width: 790px) {
  .main .container .task .left-wrapper .left-box_wrapper {
    width: 100%;
  }
}
.main .container .task .left-wrapper .left-box_wrapper .left-box {
  border: 1px solid white;
  width: 100%;
  height: 500px;
}
.main .container .task .right-wrapper {
  background: pink;
  height: 100vh;
  width: 30%;
  padding: 5px 10px;
}
@media (max-width: 790px) {
  .main .container .task .right-wrapper {
    display: none;
  }
}
.main .container .task .right-wrapper .right-box_wrapper {
  max-width: 250px;
  height: 300px;
}
@media (max-width: 1350px) {
  .main .container .task .right-wrapper .right-box_wrapper {
    width: 150px;
  }
}
.main .container .task .right-wrapper .right-box_wrapper .right-box {
  overflow-x: hidden;
  border: 1px solid white;
  width: 100%;
  height: 100%;
}
.footer {
  background: tan;
}
.footer .footer_wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 5px 0;
}
.footer .footer_wrapper .footer_links .link-git {
  display: flex;
  width: 80px;
}
.footer .footer_wrapper .footer_links .link-git:hover {
  transform: scale(1.05);
}
.footer .footer_wrapper .copyright {
  font-size: 12px;
  font-weight: 400;
}
.footer .footer_wrapper .footer_logo {
  width: 40px;
  display: flex;
}
.footer .footer_wrapper .footer_logo img {
  border-radius: 50%;
}
#burger-menu {
  display: none;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 10px;
  top: 25px;
  box-sizing: border-box;
  outline: none;
  cursor: pointer;
  border: none;
  background: none;
  width: 30px;
  height: 30px;
  -webkit-transform: translate(-50%,-50%);
  transform: translate(-50%,-50%);
}
#burger-menu+.active .box_item1 {
  -webkit-transform: translateX(0) translateY(8px) rotate(45deg);
  transform: translateX(0) translateY(8px) rotate(45deg);
}
#burger-menu+.active .box_item2 {
  opacity: 0;
}
#burger-menu+.active .box_item3 {
  -webkit-transform: translateX(0) translateY(-8px) rotate(-45deg);
  transform: translateX(0) translateY(-8px) rotate(-45deg);
}
#burger-menu .box {
  width: 30px;
  height: 4px;
  background: #7854F7;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  border-radius: 5px;
  margin: 2px 0;
  transition: .3s all ease;
}
.show_mobile_menu {
  opacity: 1 !important;
  left: 0 !important;
}
#mobile-menu {
  margin-top: 50px;
  z-index: 2;
  padding: 15px;
  box-sizing: border-box;
  opacity: 0;
  position: absolute;
  left: -100%;
  top: 0;
  width: 100%;
  background: white;
  display: flex;
  justify-content: center;
  transition: .3s all ease;
}
#mobile-menu ul {
  padding-left: 0;
  width: 300px;
  display: flex;
  flex-direction: column;
  align-items: center;
  list-style: none;
}
#mobile-menu ul li {
  text-decoration: none;
  font-size: 20px;
  font-weight: 700;
  padding: 10px 0;
  display: block;
  color: #7854F7;
  position: relative;
}
#mobile-menu .header_log-in {
  margin-top: 40px;
  text-decoration: none;
  color: black;
  font-size: 16px;
  padding: 10px 10px;
  font-weight: 500;
}
#mobile-menu .header_log-in:hover,
#mobile-menu .header_log-in:focus {
  color: #7854F7;
}
#mobile-menu .header_btn {
  border-radius: 5px;
  padding: 13px 15px;
  margin-top: 10px;
}
#mobile-menu .header_search {
  padding: 10px 10px;
  margin-top: 10px;
}
body {
  font-family: 'Roboto', sans-serif;
  overflow-x: hidden;
  background: url(../img/.png) no-repeat top center;
  font-size: 16px;
  background: bisque;
}
a {
  text-decoration: none;
  color: black;
}
.container {
  width: 100%;
  margin: 0 auto;
  padding: 0 15px;
}
.wrapper {
  display: flex;
  flex-direction: column;
  min-height: 100vh;
}
