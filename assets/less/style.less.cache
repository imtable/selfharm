.btn {
  background: gold;
  padding: 10px 15px;
  color: black;
}
.header {
  background: tan;
}
.header .header_wrapper {
  padding: 10px 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.header .header_wrapper .header_leftside .logo {
  width: 70px;
  display: flex;
}
.header .header_wrapper .header_leftside .logo img {
  border-radius: 50%;
}
.header .header_wrapper .header_middleside .menu {
  display: flex;
  justify-content: space-between;
}
